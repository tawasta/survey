<?xml version="1.0" ?>
<odoo>
   <data noupdate="1">
      <record id="mail_template_survey_response_alert" model="mail.template">
         <field name="name">Survey Response Alert Notification</field>
         <field name="model_id" ref="survey.model_survey_user_input" />
         <field name="subject">Alert - Low Feedback Score</field>
         <field name="email_from">${user.email | safe}</field>
         <field name="body_html" type="html">
<p>Hello,</p>
<p>${body_html}</p>

         </field>
         <field name="auto_delete" eval="True" />
      </record>

   </data>
</odoo>
