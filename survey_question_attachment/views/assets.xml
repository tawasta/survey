<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="assets_backend" inherit_id="web.assets_backend" name="Survey assets">
        <xpath expr="." position="inside">
            <link
                rel="stylesheet"
                href="/survey_question_attachment/static/src/css/survey_result.css"
            />
        </xpath>
    </template>
    <template
        id="assets_frontend"
        inherit_id="survey.survey_assets"
        name="Survey assets"
    >
        <xpath expr="//link[last()]" position="after">
            <link
                rel="stylesheet"
                href="/survey_question_attachment/static/src/css/survey_result_frontend.css"
            />
            <script
                type="text/javascript"
                src="/survey_question_attachment/static/src/js/survey.js"
            />
        </xpath>
    </template>
</odoo>
