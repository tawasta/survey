<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template
        id="survey_fill_form_done_hidden_scoring"
        inherit_id="survey.survey_fill_form_done"
    >
        <xpath
            expr="//t[@t-if=&quot;survey.scoring_type != 'no_scoring' and survey.scoring_success_min&quot;]"
            position="attributes"
        >
            <attribute
                name="t-if"
            >survey.scoring_type not in ['no_scoring', 'hidden_scoring'] and survey.scoring_success_min</attribute>
        </xpath>
    </template>
</odoo>
