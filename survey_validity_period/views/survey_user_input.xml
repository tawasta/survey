<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="survey_user_input_view_form_qualification" model="ir.ui.view">
        <field name="name">survey.user_input.view.form.qualification</field>
        <field name="model">survey.user_input</field>
        <field name="inherit_id" ref="survey.survey_user_input_view_form" />
        <field name="arch" type="xml">
            <field name="access_token" position="before">
                <field name="qualification_period" />
                <field
                    name="start_date"
                    attrs="{'invisible': [('qualification_period', '=', False)]}"
                />
                <field
                    name="end_date"
                    attrs="{'invisible': [('qualification_period', '=', False)]}"
                />
            </field>
        </field>
    </record>
</odoo>
