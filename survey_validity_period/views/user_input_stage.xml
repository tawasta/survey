<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="survey_user_stage_view_form" model="ir.ui.view">
        <field name="name">survey.user_input.stage.form.ended</field>
        <field name="model">survey.user_input.stage</field>
        <field name="inherit_id" ref="survey_stages.survey_user_stage_view_form" />
        <field name="arch" type="xml">
            <field name="is_cancel" position="after">
                <field name="is_end" />
            </field>
        </field>
    </record>
    <record id="survey_user_input_stage_view_tree" model="ir.ui.view">
        <field name="name">survey.user_input.stage.tree.ended</field>
        <field name="model">survey.user_input.stage</field>
        <field name="inherit_id" ref="survey_stages.survey_user_input_stage_view_tree" />
        <field name="arch" type="xml">
            <field name="is_cancel" position="after">
                <field name="is_end" />
            </field>
        </field>
    </record>
</odoo>
