#. module: survey_stages
#: model:mail.template,body_html:survey_stages.mail_template_data_participation_cancelation
msgid ""
"<?xml version=\"1.0\"?>\n"
"<div>\n"
"                    Dear ${object.partner_id.name or ''}<br/>\n"
"                    Unfortunately your participation ${object.display_name} has been canceled.\n"
"                    <br/><br/>\n"
"                    Regards,\n"
"                    % if user.signature\n"
"                        <br/>\n"
"                        ${user.signature | safe}\n"
"                    % endif\n"
"                </div>\n"
"            "
msgstr ""
"<?xml version=\"1.0\"?>\n"
"<div>\n"
"                    Hyvä ${object.partner_id.name or ''}<br/>\n"
"                    Valitettavasti osallistumisesi kyselyyn ${object.display_name} on hylätty.\n"
"                    <br/><br/>\n"
"                    Terveisin,\n"
"                    % if user.signature\n"
"                        <br/>\n"
"                        ${user.signature | safe}\n"
"                    % endif\n"
"                </div>\n"
"            "

#. module: survey_stages
#: model:mail.template,body_html:survey_stages.mail_template_data_participation_confirmation
msgid ""
"<?xml version=\"1.0\"?>\n"
"<div>\n"
"                    Dear ${object.partner_id.name or ''}<br/>\n"
"                    We are pleased to inform you that your participation ${object.display_name} has been accepted and confirmed.\n"
"                    <br/><br/>\n"
"                    Thank you,\n"
"                    % if user.signature\n"
"                        <br/>\n"
"                        ${user.signature | safe}\n"
"                    % endif\n"
"                </div>\n"
"            "
msgstr ""
"<?xml version=\"1.0\"?>\n"
"<div>\n"
"                    Hyvä ${object.partner_id.name or ''}<br/>\n"
"                    Voimme iloiksemme vahvistaa, että osallistumisesi kyselyyn ${object.display_name} on hyväksytty ja vahvistettu.\n"
"                    <br/><br/>\n"
"                    Kiitos,\n"
"                    % if user.signature\n"
"                        <br/>\n"
"                        ${user.signature | safe}\n"
"                    % endif\n"
"                </div>\n"
"            "

#. module: survey_stages
#: model:mail.template,body_html:survey_stages.mail_template_data_participation_sent
msgid ""
"<?xml version=\"1.0\"?>\n"
"<div>\n"
"                    Dear ${object.partner_id.name or ''}<br/>\n"
"                    Your survey answer has been completed and received.\n"
"                    <br/><br/>\n"
"                    Thank you,\n"
"                    % if user.signature\n"
"                        <br/>\n"
"                        ${user.signature | safe}\n"
"                    % endif\n"
"                </div>\n"
"            "
msgstr ""
"<?xml version=\"1.0\"?>\n"
"<div>\n"
"                    Hyvä ${object.partner_id.name or ''}<br/>\n"
"                    Kyselyn vastauksesi on vahvistettu ja vastaanotettu.\n"
"                    <br/><br/>\n"
"                    Kiitos,\n"
"                    % if user.signature\n"
"                        <br/>\n"
"                        ${user.signature | safe}\n"
"                    % endif\n"
"                </div>\n"
"            "

#. module: survey_stages
#: model:mail.template,body_html:survey_stages.mail_template_data_participation_draft
msgid ""
"<?xml version=\"1.0\"?>\n"
"<div>\n"
"                    Dear ${object.partner_id.name or ''}<br/>\n"
"                    Your survey answer has been saved as a draft. In order to complete your answer please remember to send it.\n"
"                    <br/><br/>\n"
"                    Thank you,\n"
"                    % if user.signature\n"
"                        <br/>\n"
"                        ${user.signature | safe}\n"
"                    % endif\n"
"                </div>\n"
"            "
msgstr ""
"<?xml version=\"1.0\"?>\n"
"<div>\n"
"                    Hyvä ${object.partner_id.name or ''}<br/>\n"
"                    Kyselyn vastauksesi on tallennettu luonnoksenta. Vahvistaaksesi vastauksesi muista lähettää se.\n"
"                    <br/><br/>\n"
"                    Kiitos,\n"
"                    % if user.signature\n"
"                        <br/>\n"
"                        ${user.signature | safe}\n"
"                    % endif\n"
"                </div>\n"
"            "

