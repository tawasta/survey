"                                    <td style=\"text-align: center; font-size: 14px;\">\n"
"                                        Sent by <a target=\"_blank\" href=\"${survey.company_id.website}\" style=\"color: #875A7B;\">${survey.company_id.name}</a>\n"
"                                    </td>\n"
"                                </tr>\n"
"                            </table>\n"
"                            % endif\n"
"                        </td>\n"
"                    </tr>\n"
"                </table>\n"
